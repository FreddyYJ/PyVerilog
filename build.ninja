################################################################################
# Autoupdate this build.ninja from build.py.

rule autoupdate
  command = python3 $in
  generator = 1
build build.ninja: autoupdate build.py


################################################################################
# Rules

rule compile_cpp
  command = g++ -rdynamic -g ${opt} -std=gnu++2a ${includes} -MMD -MF $
      ${out}.d -c ${in} -o ${out}
  depfile = ${out}.d
  deps = gcc
rule compile_c
  command = gcc -rdynamic -g ${opt} ${includes} -MMD -MF ${out}.d -c ${in} $
      -o ${out}
  depfile = ${out}.d
  deps = gcc
rule static_lib
  command = ar rcs ${out} ${in} > /dev/null
rule link
  command = g++ -rdynamic -g $opt ${in} -Wl,--whole-archive ${local_libs} $
      -Wl,--no-whole-archive ${global_libs} -o ${out}
rule metron
  command = bin/metron -q -r ${src_dir} -o ${dst_dir} -c ${src_top}
rule verilator
  command = verilator ${includes} --cc ${src_top} -Mdir ${dst_dir}
rule make
  command = make --quiet -C ${dst_dir} -f ${makefile} > /dev/null
rule run_test
  command = ${in} | grep "All tests pass." && touch ${out}
rule console
  command = ${in}
  pool = console
rule iverilog
  command = iverilog -g2012 ${defines} ${includes} ${in} -o ${out}
rule yosys
  command = yosys -q -p 'read_verilog ${includes} -sv ${in}; dump; $
      synth_ice40 -json ${out};'
rule nextpnr-ice40
  command = nextpnr-ice40 -q --${chip} --package ${package} --json ${in} $
      --asc ${out} --pcf ${pcf}
rule icepack
  command = icepack ${in} ${out}


